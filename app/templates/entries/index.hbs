<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Age</th>
      <th>Bust</th>
      <th>Waist</th>
      <th>Hip</th>
      <th>Address</th>
      <th>E-mail</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |entry|}}
    <tr>
      <th scope="row">1</th>
      <td>{{entry.firstName}}</td>
      <td>{{entry.lastName}}</td>
      <td>{{entry.age}}</td>
      <td>{{entry.bust}}</td>
      <td>{{entry.waist}}</td>
      <td>{{entry.hip}}</td>
      <td>{{entry.address}}</td>
      <td>{{entry.email}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>